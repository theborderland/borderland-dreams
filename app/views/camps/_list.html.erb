<%# app/views/articles/_list.html.erb %>
<div id="filterrific_results">

  <table>
    <tr>
      <th>Name</th>
      <th>Created at</th>
    </tr>
    <% camps.each do |camp| %>
      <tr>
        <td><%= link_to(camp.name, camp_path(camp)) %></td>
        <td><%= camp.created_at %></td>
      </tr>
    <% end %>
  </table>
</div>
